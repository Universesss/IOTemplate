{
	"env": {
		"vex_compilerPath": "${config:vexrobotics.vexcode.Cpp.Toolchain.Path}\\clang\\bin\\clang.exe",
		"vex_sdkPath": "${config:vexrobotics.vexcode.Cpp.Sdk.Home}\\V5\\V5_20240802_15_00_00\\vexv5",
		"vex_gcc": "${vex_sdkPath}/gcc/include/c++/4.9.3",
		"vex_sdkIncludes": [
			"${vex_sdkPath}/clang/8.0.0/include/**",
			"${vex_gcc}/**",
			"${vex_gcc}/arm-none-eabi/thumb",
			"${vex_sdkPath}/gcc/include",
			"${vex_sdkPath}/include/**",
			"${workspaceFolder}/include/**",
			"${workspaceFolder}/src/**",
			"${vex_sdkPath}/clang/8.0.0/include",
			"${vex_gcc}",
			"${vex_sdkPath}/include",
			"${workspaceFolder}/include",
			"${workspaceFolder}/src",
			"C:/Program Files (x86)/VEX Robotics/VEXcode Pro V5/sdk/vexv5/include",
			"C:/Program Files (x86)/VEX Robotics/VEXcode Pro V5/sdk/vexv5/gcc/include",
			"C:/Program Files (x86)/VEX Robotics/VEXcode Pro V5/sdk/vexv5/gcc/include/c++/4.9.3"
		]
	},
	"configurations": [
		{
			"name": "Win32",
			"includePath": [
				"${vex_sdkIncludes}"
			],
			"compilerPath": "${vex_compilerPath}",
			"cStandard": "gnu99",
			"cppStandard": "gnu++17",
			"intelliSenseMode": "windows-clang-x64",
			"defines": [
				"__VEX_V5__",
				"__clang__"
			]
		},
		{
			"name": "Mac",
			"includePath": [
				"${vex_sdkIncludes}"
			],
			"compilerPath": "${vex_compilerPath}",
			"cStandard": "gnu99",
			"cppStandard": "gnu++17",
			"intelliSenseMode": "macos-clang-x86",
			"defines": [
				"__VEX_V5__",
				"__clang__"
			]
		},
		{
			"name": "Linux",
			"includePath": [
				"${vex_sdkIncludes}"
			],
			"compilerPath": "",
			"cStandard": "gnu99",
			"cppStandard": "gnu++17",
			"intelliSenseMode": "linux-clang-x86",
			"defines": [
				"__VEX_V5__",
				"__clang__"
			]
		}
	],
	"version": 4
}